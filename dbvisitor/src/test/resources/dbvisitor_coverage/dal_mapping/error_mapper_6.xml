<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//dbvisitor.net//DTD Mapper 1.0//EN" "https://www.dbvisitor.net/schema/dbvisitor-mapper.dtd">
<mapper namespace="resultMap_test">
    <entity table="blob_resource" type="net.hasor.test.entity.BlobResourceV5" comment="表备注">
        <id column="id" property="id" dbType="bigint" keyType="auto" other="auto_increment" comment="ID列"/>
        <mapping column="gmt_create" property="gmtCreate" dbType="timestamp" nullable="false" defaultValue="current_timestamp" comment="创建时间"/>
        <mapping column="gmt_modified" property="gmtModified" dbType="timestamp" nullable="false" defaultValue="current_timestamp" other="on update current_timestamp" comment="修改时间"/>
        <mapping column="instance_id" property="instanceId" dbType="varchar" length="64" nullable="false" comment="实例ID"/>
        <mapping column="owner_name" property="ownerName" dbType="varchar(128)"/>
        <mapping column="owner_type" property="ownerType" dbType="varchar" length="32"/>
        <mapping column="content" property="content" length="32"/>
        <index name="uk_b" unique="true">
            <column>instanceId</column>
        </index>
        <index name="idx_a" columns="gmt_modified,instanceId"/>
        <index columns="content,owner_name"/> <!-- error -->
    </entity>
</mapper>