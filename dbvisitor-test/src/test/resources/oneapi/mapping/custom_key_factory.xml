<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//dbvisitor.net//DTD Mapper 1.0//EN"
        "https://www.dbvisitor.net/schema/dbvisitor-mapper.dtd">
<mapper namespace="net.hasor.test.xml.customkey">

    <!-- 使用自定义 GeneratedKeyHandlerFactory 类名作为 keyType -->
    <entity id="CustomKeyEntity" table="user_info" type="net.hasor.dbvisitor.test.oneapi.model.UserInfo">
        <id column="id" property="id"
            keyType="net.hasor.dbvisitor.mapping.keyseq.Uuid32KeySeqHolderFactory"/>
        <mapping column="name" property="name"/>
    </entity>

</mapper>
