<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//dbvisitor.net//DTD Mapper 1.0//EN"
        "https://www.dbvisitor.net/schema/dbvisitor-mapper.dtd">
<mapper namespace="net.hasor.test.xml.javatype.err">

    <!-- javaType 与属性类型不兼容 → ClassCastException -->
    <entity id="BadJavaTypeEntity" table="user_info" type="net.hasor.dbvisitor.test.model.UserInfo">
        <id column="id" property="id" javaType="java.lang.String"/>
        <mapping column="name" property="name"/>
    </entity>

</mapper>
